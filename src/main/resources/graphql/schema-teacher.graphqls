type Teacher {
  id: ID!
  firstName: String!
  lastName: String!
  email: String!
  dob: String!
  coursesTaught: [ID],
  address: Address
}

type Address {
  city: String!
  state: String!
  country: String!
}

input TeacherInput {
  firstName: String!
  lastName: String!
  dob: String!
  email: String!
}

extend type Query {
  getAllTeachers: [Teacher]
  getTeacherById(id: ID!): Teacher
}

extend type Mutation {
  createTeacher(teacher: TeacherInput!): Teacher
}