type Teacher {
  id: ID!
  firstName: String!
  lastName: String!
  email: String!
  dob: String!
  gender: Gender!
  classes: [String],
  address: Address
}

type Address {
  city: String!
  state: String!
  country: String!
}

input TeacherInput {
  name: String!
  dob: String!
  email: String!
}

enum Gender {
  MALE,
  FEMALE,
}

type Query {
  getAllTeachers: [Teacher]
  getTeacherById(id: ID!): Teacher
}

type Mutation {
  createTeacher(teacher: TeacherInput!): Teacher
}