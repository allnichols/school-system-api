type Course {
    id: ID!
    courseName: String!
    gradeLevel: Int!
    teacher: Teacher
}

type Query {
    getAllCourses: [Course]
    getCourseById(id: ID!): Course
}

input CourseInput {
    courseName: String!
    gradeLevel: Int!
}

type Mutation {
    createCourse(course: CourseInput!): Course
}