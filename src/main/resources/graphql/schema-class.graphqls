type Class {
    id: ID!
    name: String!
    teacher: Teacher!
    students: [Student]
    subject: String!
    gradeLevel: Int!
}

type Query {
    getAllClasses: [Class]
    getClassById(id: ID!): Class
}

type Mutation {
    createClass(name: String!, teacherId: ID!, subject: String!, gradeLevel: Int!): Class
    addStudentToClass(classId: ID!, studentId: ID!): Class
    removeStudentFromClass(classId: ID!, studentId: ID!): Class
    removeClass(id: ID!): Class
}